<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ars15.com.mycalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".prefix"
            android:label="Prefix Calculator"/>
        <activity android:name=".postfix"
            android:label="Postfix Calculator"/>
        <activity android:name=".infix"
            android:label="Infix Calculator"/>
        <activity android:name=".convert"
            android:label="Converter"/>
    </application>

</manifest>