<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/prefix"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="ars15.com.mycalc.prefix"
    android:weightSum="1"
    android:hapticFeedbackEnabled="true"
    android:soundEffectsEnabled="true">

    <TextView
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="Prefix Calculator.Enter Prefix expression\n"
        android:textSize="16sp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:id="@+id/textView"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView

        android:paddingBottom="0dp"
        android:layout_width="0dp"
        android:layout_height="85dp"
        android:textAlignment="textEnd"
        android:textSize="24dp"
        android:hint="\nEnter Expression     "
        android:id="@+id/editText"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline" />

    <Button
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button2"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        app:layout_constraintRight_toLeftOf="@+id/button3"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="14dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:text="5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintRight_toRightOf="@+id/button9"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button9" />

    <Button
        android:text="6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="11dp"
        app:layout_constraintRight_toRightOf="@+id/textView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/button3" />

    <Button
        android:text="7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button8"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button13"
        app:layout_constraintRight_toRightOf="@+id/button5"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="@+id/button5" />

    <Button
        android:text="8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button9"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintRight_toRightOf="@+id/button12"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="@+id/button12" />

    <Button
        android:text="9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button10"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintRight_toRightOf="@+id/button7"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="@+id/button7" />

    <Button
        android:text="-"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button11"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintRight_toRightOf="@+id/button10"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/button10" />

    <Button
        android:text="0"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button14"
        app:layout_constraintRight_toRightOf="@+id/button14"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/button14" />

    <Button
        android:text="+"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button15"
        app:layout_constraintRight_toRightOf="@+id/button8"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/button8" />

    <Button
        android:text="4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintRight_toRightOf="@+id/button2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button2" />

    <Button
        android:text="3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:text="/"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/button15"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="110dp" />

    <Button
        android:text="="
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button16"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/button"
        android:layout_marginTop="4dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/dotbutton" />

    <Button
        android:text="*"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button14"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/dotbutton"
        app:layout_constraintRight_toRightOf="@+id/dotbutton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/dotbutton" />

    <Button
        android:text="2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintRight_toRightOf="@+id/editText"
        app:layout_constraintLeft_toLeftOf="@+id/editText"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:text="Space"
        android:layout_width="333dp"
        android:layout_height="50dp"
        android:id="@+id/button17"
        android:layout_below="@+id/editText"
        android:layout_toEndOf="@+id/textView"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/powerbutton"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:text="C"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button16"
        app:layout_constraintRight_toRightOf="@+id/button11"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="4dp"
        app:layout_constraintLeft_toLeftOf="@+id/button11" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="20dp"
        tools:layout_editor_absoluteX="0dp"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true" />

    <TextView
        android:text="         "
        android:layout_width="0dp"
        android:id="@+id/textView2"
        android:layout_height="45dp"
        android:textSize="18sp"
        android:textAlignment="textEnd"
        android:hapticFeedbackEnabled="true"
        android:soundEffectsEnabled="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="6dp"
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        android:layout_marginEnd="6dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="102dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="99dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:text="."
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/dotbutton"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button17"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:text="^"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/powerbutton"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button15"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>